@model SellerSignupViewModel

<div class="form-group">
	<label asp-for="Seller.Name" class="control-label"></label>
	<input asp-for="Seller.Name" class="form-control" />
	<span asp-validation-for="Seller.Name" class="text-danger"></span>
</div>
<div class="form-group">
	<label asp-for="Seller.PhoneNumber" class="control-label"></label>
	<input asp-for="Seller.PhoneNumber" class="form-control" />
	<span asp-validation-for="Seller.PhoneNumber" class="text-danger"></span>
</div>
<div class="form-row">
	<div class="form-group col-md-6">
		<label asp-for="Seller.Email" class="control-label"></label>
		<input asp-for="Seller.Email" class="form-control" />
		<span asp-validation-for="Seller.Email" class="text-danger"></span>
	</div>
	<div class="form-group col-md-6">
		<div class="form-row">
			<label asp-for="Seller.Password" class="control-label"></label>
			<input asp-for="Seller.Password" class="form-control" />
			<span asp-validation-for="Seller.Password" class="text-danger"></span>
		</div>
		<div class="form-row">
			<label asp-for="ConfirmPassword" class="control-label"></label>
			<input asp-for="ConfirmPassword" class="form-control" />
			<span asp-validation-for="ConfirmPassword" class="text-danger"></span>
		</div>
	</div>
</div>
<input asp-for="ReturnUrl" />